		"CHANGELOG.md",
		"LICENSE",
		"README.md",
		"index.js",
		"scss.js"
	],
	"main": "index.js",
	"dependencies": {
		"stylelint-config-recommended": "^13.0.0",
		"stylelint-config-recommended-scss": "^13.0.0",
		"stylelint-stylistic": "^0.4.3"
	},
	"peerDependencies": {
		"stylelint": "^15.10.3"
	},
	"npmpackagejsonlint": {
		"extends": "@wordpress/npm-package-json-lint-config",
		"rules": {
			"valid-values-license": [
				"error",
				[
					"MIT"
				]
			]
		}
	},
	"publishConfig": {
		"access": "public"
	}
