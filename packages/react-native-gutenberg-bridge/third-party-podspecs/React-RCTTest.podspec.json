{
  "name": "React-RCTTest",
  "version": "0.60.0-patched",
  "summary": "Tools for integration and snapshot testing.",
  "homepage": "http://facebook.github.io/react-native/",
  "license": "MIT",
  "authors": "Facebook, Inc. and its affiliates",
  "platforms": {
    "ios": "9.0",
    "tvos": "9.2"
  },
  "source": {
    "git": "https://github.com/jtreanor/react-native.git",
    "tag": "v0.60.0-patched"
  },
  "source_files": "**/*.{h,m}",
  "preserve_paths": [
    "package.json",
    "LICENSE",
    "LICENSE-docs"
  ],
  "frameworks": "XCTest",
  "header_dir": "RCTTest",
  "dependencies": {
    "React-Core": [
      "0.60.0-patched"
    ]
  },
  "prepare_command": "TMP_DIR=$(mktemp -d); mv * $TMP_DIR; cp -R \"$TMP_DIR/RNTester/RCTTest\"/* . && true"
}
