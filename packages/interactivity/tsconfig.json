{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"rootDir": "src",
		"declarationDir": "build-types",

		"strictNullChecks": false,
		"strictPropertyInitialization": false,

		"noImplicitThis": false,
		"noImplicitAny": false
	},
	"include": [ "src/**/*" ]
}
