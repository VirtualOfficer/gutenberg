{
	"name": "@wordpress/react-native-gutenberg-editor",
	"version": "1.12.0",
	"description": "The Gutenberg native editor for Android and iOS.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress",
		"editor",
		"native",
		"ios",
		"android",
		"mobile"
	],
	"homepage": "https://github.com/WordPress/gutenberg/tree/master/packages/react-native-gutenberg-editor/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/WordPress/gutenberg.git",
		"directory": "packages/react-native-gutenberg-editor"
	},
	"bugs": {
		"url": "https://github.com/WordPress/gutenberg/issues"
	},
	"main": "build/index.js",
	"module": "build-module/index.js",
	"react-native": "src/index",
	"dependencies": {
		"@babel/runtime": "^7.3.1",
		"@wordpress/a11y": "file:../a11y",
		"@wordpress/annotations": "file:../annotations",
		"@wordpress/api-fetch": "file:../api-fetch",
		"@wordpress/autop": "file:../autop",
		"@wordpress/blob": "file:../blob",
		"@wordpress/block-directory": "file:../block-directory",
		"@wordpress/block-editor": "file:../block-editor",
		"@wordpress/block-library": "file:../block-library",
		"@wordpress/block-serialization-default-parser": "file:../block-serialization-default-parser",
		"@wordpress/block-serialization-spec-parser": "file:../block-serialization-spec-parser",
		"@wordpress/blocks": "file:../blocks",
		"@wordpress/components": "file:../components",
		"@wordpress/compose": "file:../compose",
		"@wordpress/core-data": "file:../core-data",
		"@wordpress/data": "file:../data",
		"@wordpress/data-controls": "file:../data-controls",
		"@wordpress/date": "file:../date",
		"@wordpress/deprecated": "file:../deprecated",
		"@wordpress/dom": "file:../dom",
		"@wordpress/dom-ready": "file:../dom-ready",
		"@wordpress/edit-post": "file:../edit-post",
		"@wordpress/edit-widgets": "file:../edit-widgets",
		"@wordpress/editor": "file:../editor",
		"@wordpress/element": "file:../element",
		"@wordpress/escape-html": "file:../escape-html",
		"@wordpress/format-library": "file:../format-library",
		"@wordpress/hooks": "file:../hooks",
		"@wordpress/html-entities": "file:../html-entities",
		"@wordpress/i18n": "file:../i18n",
		"@wordpress/is-shallow-equal": "file:../is-shallow-equal",
		"@wordpress/keycodes": "file:../keycodes",
		"@wordpress/list-reusable-blocks": "file:../list-reusable-blocks",
		"@wordpress/media-utils": "file:../media-utils",
		"@wordpress/notices": "file:../notices",
		"@wordpress/nux": "file:../nux",
		"@wordpress/plugins": "file:../plugins",
		"@wordpress/priority-queue": "file:../priority-queue",
		"@wordpress/react-native-aztec": "file:../react-native-aztec",
		"@wordpress/react-native-gutenberg-bridge": "file:../react-native-gutenberg-bridge",
		"@wordpress/redux-routine": "file:../redux-routine",
		"@wordpress/rich-text": "file:../rich-text",
		"@wordpress/shortcode": "file:../shortcode",
		"@wordpress/token-list": "file:../token-list",
		"@wordpress/url": "file:../url",
		"@wordpress/viewport": "file:../viewport",
		"@wordpress/wordcount": "file:../wordcount",
		"babel-plugin-react-native-classname-to-style": "1.2.2",
		"babel-plugin-react-native-platform-specific-extensions": "1.1.1",
		"jsc-android": "^241213.1.0",
		"jsdom-jscore-rn": "git+https://github.com/iamcco/jsdom-jscore-rn.git#a562f3d57c27c13e5bfc8cf82d496e69a3ba2800",
