{
	"apiVersion": 2,
	"name": "core/marquee",
	"category": "text",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "marquee",
			"default": ""
		},
		"direction": {
			"type": "string",
			"default": "left"
		},
		"behavior": {
			"type": "string",
			"default": "scroll"
		},
		"loop": {
			"type": "number",
			"default": "INFINITE"
		}
	},
	"supports": {
		"anchor": true,
		"className": false,
		"color": {
			"link": true
		},
		"fontSize": true,
		"lineHeight": true,
		"__experimentalFontFamily": true
	}
}
