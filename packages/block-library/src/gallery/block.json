{
	"name": "core/gallery",
	"category": "media",
	"attributes": {
		"ids": {
			"type": "array",
			"items": {
				"type": "number"
			},
			"default": []
		},
		"columns": {
			"type": "number",
			"minimum": 1,
			"maximum": 8
		},
		"caption": {
			"type": "string",
			"source": "html",
			"selector": ".blocks-gallery-caption"
		},
		"imageCrop": {
			"type": "boolean",
			"default": true
		},
		"linkTo": {
			"type": "string"
		},
		"sizeSlug": {
			"type": "string",
			"default": "large"
		},
		"imageSettings": {
			"type": "object",
			"default": {
				"allowResize": false,
				"allowAlign": false
			}
		}
	},
	"providesContext": {
		"gallery/imageSettings": "imageSettings"
	},
	"supports": {
		"anchor": true,
		"align": true
	}
}
