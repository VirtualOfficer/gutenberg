{
	"name": "core/gallery",
	"category": "media",
	"attributes": {
		"imageUploads": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object"
			}
		},
		"columns": {
			"type": "number",
			"minimum": 1,
			"maximum": 8
		},
		"caption": {
			"type": "string",
			"source": "html",
			"selector": ".blocks-gallery-caption"
		},
		"imageCrop": {
			"type": "boolean",
			"default": true
		},
		"linkTo": {
			"type": "string"
		},
		"linkTarget": {
			"type": "string"
		},
		"sizeSlug": {
			"type": "string",
			"default": "large"
		},
		"allowAlign": {
			"type": "boolean",
			"default": false
		},
		"allowResize": {
			"type": "boolean",
			"default": false
		},
		"isListItem": {
			"type": "boolean",
			"default": true
		},
		"applyToAllImages": {
			"type": "boolean",
			"default": false
		}
	},
	"providesContext": {
		"allowAlign": "allowAlign",
		"allowResize": "allowResize",
		"isListItem": "isListItem",
		"applyToAllImages": "applyToAllImages",
		"galleryLinkDestination": "linkTo",
		"galleryLinkTarget": "linkTarget",
		"gallerySizeSlug": "sizeSlug"
	},
	"supports": {
		"anchor": true,
		"align": true
	}
}
