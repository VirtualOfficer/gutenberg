{
	"apiVersion": 2,
	"name": "core/pullquote",
	"category": "text",
	"attributes": {
		"value": {
			"type": "string",
			"source": "html",
			"selector": "blockquote",
			"multiline": "p",
			"role": "content"
		},
		"citation": {
			"type": "string",
			"source": "html",
			"selector": "cite",
			"default": "",
			"role": "content"
		},
		"mainColor": {
			"type": "string"
		},
		"customMainColor": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		},
		"customTextColor": {
			"type": "string"
		}
	},
	"supports": {
		"anchor": true,
		"align": [ "left", "right", "wide", "full" ]
	},
	"editorStyle": "wp-block-pullquote-editor",
	"style": "wp-block-pullquote"
}
