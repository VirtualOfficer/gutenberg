{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"__experimental": true,
	"name": "core/breadcrumbs",
	"title": "Breadcrumbs",
	"category": "design",
	"description": "Displays breadcrumbs of a page's hierarchy",
	"textdomain": "default",
	"usesContext": [ "postId", "postType" ],
	"attributes": {
		"nestingLevel": {
			"type": "number",
			"default": 0
		},
		"separator": {
			"type": "string",
			"default": "/"
		},
		"showCurrentPageTitle": {
			"type": "boolean",
			"default": false
		},
		"showLeadingSeparator": {
			"type": "boolean",
			"default": false
		},
		"showSiteTitle": {
			"type": "boolean",
			"default": true
		},
		"siteTitleOverride": {
			"type": "string"
		},
		"textAlign": {
			"type": "string"
		}
	},
	"supports": {
		"align": true,
		"color": {
			"gradients": true,
			"link": true
		},
		"html": false,
		"spacing": {
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		}
	},
	"editorStyle": "wp-block-breadcrumbs-editor",
	"style": "wp-block-breadcrumbs"
}
