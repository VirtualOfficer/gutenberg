{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "core/tab",
	"title": "Tab",
	"category": "design",
	"description": "Single tab within a tabs block.",
	"textdomain": "default",
	"__experimental": true,
	"attributes": {
		"isActive": {
			"type": "boolean",
			"default": false
		},
		"label": {
			"type": "string",
			"default": ""
		},
		"slug": {
			"type": "string",
			"default": ""
		},
		"tabIndex": {
			"type": "number"
		}
	},
	"parent": [ "core/tabs" ],
	"supports": {
		"anchor": true,
		"html": false,
		"reusable": false
	},
	"style": "wp-block-tab"
}
