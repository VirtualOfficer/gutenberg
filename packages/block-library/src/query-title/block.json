{
	"apiVersion": 2,
	"name": "core/query-title",
	"title": "Query Title",
	"category": "design",
	"description": "Display the query title based on the queried object. Supports search, archive and 404 templates. Dynamic content is available on the search template for: %search%, %total%.",
	"textdomain": "default",
	"usesContext": [ "templateSlug" ],
	"attributes": {
		"content": {
			"type": "string",
			"default": "Query title"
		},
		"textAlign": {
			"type": "string"
		},
		"level": {
			"type": "number",
			"default": 1
		}
	},
	"supports": {
		"align": [ "wide", "full" ],
		"html": false,
		"color": {
			"gradients": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true
		}
	},
	"editorStyle": "wp-block-query-title-editor"
}
